#include <stdio.h>
#include <stdlib.h>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
int main(int argc, char *argv[]) {
	// Print Hello World! on the screen
	printf(Hello World!";
	// The program pauses to see the console output
	system("PAUSE");
	return 0;
}
